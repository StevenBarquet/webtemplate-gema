  "[json]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "adel√°ntate",
    "Barquet",
    "facebook",
    "Fcol",
    "forgemytech",
    "logearte",
    "Qwik",
    "spanish",
    "trpc",
    "webp"
  ],"material-icon-theme.folders.associations": {
    "store": "Redux-store",
    "Store": "Redux-store",
    "appConfig": "config",
    "AppConfig": "config",
    "Providers": "context",
    "providers": "context",
    "GlobalLayout": "layout",
    "globalLayout": "layout",
    "authRouter": "secure",
    "AuthRouter": "secure",
    "publicRouter": "public",
    "tRPC": "cluster",
    "TRPC": "cluster",
    "trcp": "cluster",
    "services3rdP": "connection",
